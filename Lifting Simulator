local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()
local Window = Rayfield:CreateWindow({
    Name = "DragonHub | By Toothless The Night Fury#9772",
    LoadingTitle = "DragonHub",
    LoadingSubtitle = "By Toothless The Night Fury#9772",
    ConfigurationSaving = {
        Enabled = true,
        FileName = "DragonHub",
},
    KeySystem = true,
    KeySettings = {
	Title = "DragonHub",
	Subtitle = "Key System",
	Note = "Join the discord (https://discord.gg/KwyPcJQ9ma)",
	Key = "8HXCqS4h58xUg7IhfU0Rw6W9V8bLuj25mLx6fnizvfQIU8T93BmOW95r7CWab54QWSqfAAtX82SAX5zW0li7164a643ygrhR6Z2"
}
})

-- Values

_G.autoLift = true
_G.autoSell = true

-- Tabs

local FarmingTab = Window:CreateTab("Farming Menu")

-- Toggle

local autoLiftToggle = FarmingTab:CreateToggle({
	Name = "Auto Lift Muscle!",
	CurrentValue = false,
	Flag = "Toggle1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
	Callback = function(Value)
		_G.autoLift = Value
		autoLift()
	end,
})

local autoSellToggle = FarmingTab:CreateToggle({
	Name = "Auto Sell Muscle!",
	CurrentValue = false,
	Flag = "Toggle2", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
	Callback = function(Value)
		_G.autoSell = Value
		autoSell()
	end,
})

-- Functions

function autoLift()
    while _G.autoLift == true do
		local args = {
			[1] = {
			[1] = "GainMuscle"}}
		game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
		task.wait()
	end
end

function autoSell()
    while _G.autoSell == true do
		local args = {
			[1] = {
			[1] = "SellMuscle"}}
		game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
		task.wait(15)
	end
end
