local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Player = game.Players.LocalPlayer
local Window = OrionLib:MakeWindow({Name = "DragonHub Key System", HidePremium = false, SaveConfig = true, IntroText = "DragonHub Key System"})


OrionLib:MakeNotification({
	Name = "Logged In!",
	Content = "You are logged in as "..Player.Name.."!",
	Image = "rbxassetid://4483345998",
	Time = 5
})

_G.Key = "Qdx$0!XAOkcf&Wx+3YjB,a`L6WJzRD"
_G.KeyInput = "string"

function MakeScriptHub()
	local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
	local Window = OrionLib:MakeWindow({Name = "DragonHub | By Toothless The Night Fury#9772", HidePremium = true, IntroText = "DragonHub", IntroEnabled = false, SaveConfig = true, ConfigFolder = "OrionTest"})
	-- SCRIPT GOES HERE--

	-- Values
	_G.autoLift = true
	_G.autoSell = true

	--Buy Item Values


	-- Equip Item Values


	-- Tabs
	local LiftingTab = Window:MakeTab({
        Name = "Farming Menu",
        Icon = "rbxassetid://4688175782",
        PremiumOnly = false
    })

	-- Functions
	function autoLift()
		while _G.autoLift == true do
			local args = {
				[1] = {
				[1] = "GainMuscle"}}
			game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
			task.wait()
		end
	end

	function autoSell()
		while _G.autoSell == true do
			local args = {
				[1] = {
				[1] = "SellMuscle"}}
			game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
			task.wait()
		end
	end

	-- Toggles

	local autoLiftToggle = LiftingTab:AddToggle({
        Name = "Auto Lift",
        Default = false,
        Callback = function(Value)
            _G.autoLift = Value
            autoLift()
        end
    })

	local autoSellToggle = LiftingTab:AddToggle({
        Name = "Auto Sell",
        Default = false,
        Callback = function(Value)
            _G.autoSell = Value
            autoSell()
        end
    })

	-- Buttons


end

function CorrectKeyNotification()
	OrionLib:MakeNotification({
		Name = "Correct Key!",
		Content = "You have entered the correct key!",
		Image = "rbxassetid://4483345998",
		Time = 5
	})
end

function IncorrectKeyNotification()
	OrionLib:MakeNotification({
		Name = "Incorrect Key!",
		Content = "You have entered the Incorrect key!",
		Image = "rbxassetid://4483345998",
		Time = 5
	})
end

function DiscordNotification()
	OrionLib:MakeNotification({
		Name = "Discord Copied!",
		Content = "The Discord Link has been copied to clipboard!",
		Image = "rbxassetid://4483345998",
		Time = 5
	})
end

local Tab = Window:MakeTab({
	Name = "Key",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddTextbox({
	Name = "Enter Key",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		_G.KeyInput = (Value)
	end
})

Tab:AddButton({
	Name = "Check Key!",
		Callback = function()
      		if _G.KeyInput == _G.Key then
				MakeScriptHub()
				CorrectKeyNotification()
			else
				IncorrectKeyNotification()
		end
    end
})

Tab:AddButton({
	Name = "Get Key Here!",
		Callback = function()
            if setclipboard("https://discord.gg/KwyPcJQ9ma") then
                DiscordNotification()
            end
		end
})
