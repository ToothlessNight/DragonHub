    function autoBoss()
        while _G.autoBoss == true do
            game.ReplicatedStorage.Events.StartBossFight:FireServer()
            wait(0.5)
            local args = {
                [1] = workspace["Magma Boss"]
            }
            
            game:GetService("ReplicatedStorage").Events.Fire:FireServer(unpack(args))
            wait(5)
        end
    end

